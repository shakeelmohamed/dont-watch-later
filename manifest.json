{
  "name": "Donâ€™t Watch Later",
  "version": "0.3.0",
  "description": "A simple extension to disable autoplay for videos from your YouTube watch later playlist.",
  "icons": {
    "128": "x-icon-128.png"
  },
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://*.youtube.com/watch*list=wl*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "manifest_version": 3
}
